<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="oa-common-provider" owner="programmer" organization="dubbox"/>
    <dubbo:registry address="zookeeper://localhost:2181"/>
    <dubbo:protocol name="rest" port="8080" threads="500" server="jetty" accepts="500"/>
                    <!--extension="com.alibaba.dubbo.rpc.protocol.rest.support.LoggingFilter"/>-->
    <dubbo:service interface="com.mwee.bpm.oa.service.api.CommonService" ref="commonService"/>
</beans>